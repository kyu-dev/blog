#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/arthurvienotongaro/Developer/projets/blog/node_modules/.pnpm/sanity@3.89.0_@emotion+is-prop-valid@1.2.2_@types+node@22.15.21_@types+react@18.3.22_im_a81e1bea9f6e0990b34fbe0bbb3d8d38/node_modules/sanity/bin/node_modules:/Users/arthurvienotongaro/Developer/projets/blog/node_modules/.pnpm/sanity@3.89.0_@emotion+is-prop-valid@1.2.2_@types+node@22.15.21_@types+react@18.3.22_im_a81e1bea9f6e0990b34fbe0bbb3d8d38/node_modules/sanity/node_modules:/Users/arthurvienotongaro/Developer/projets/blog/node_modules/.pnpm/sanity@3.89.0_@emotion+is-prop-valid@1.2.2_@types+node@22.15.21_@types+react@18.3.22_im_a81e1bea9f6e0990b34fbe0bbb3d8d38/node_modules:/Users/arthurvienotongaro/Developer/projets/blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/arthurvienotongaro/Developer/projets/blog/node_modules/.pnpm/sanity@3.89.0_@emotion+is-prop-valid@1.2.2_@types+node@22.15.21_@types+react@18.3.22_im_a81e1bea9f6e0990b34fbe0bbb3d8d38/node_modules/sanity/bin/node_modules:/Users/arthurvienotongaro/Developer/projets/blog/node_modules/.pnpm/sanity@3.89.0_@emotion+is-prop-valid@1.2.2_@types+node@22.15.21_@types+react@18.3.22_im_a81e1bea9f6e0990b34fbe0bbb3d8d38/node_modules/sanity/node_modules:/Users/arthurvienotongaro/Developer/projets/blog/node_modules/.pnpm/sanity@3.89.0_@emotion+is-prop-valid@1.2.2_@types+node@22.15.21_@types+react@18.3.22_im_a81e1bea9f6e0990b34fbe0bbb3d8d38/node_modules:/Users/arthurvienotongaro/Developer/projets/blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/sanity" "$@"
else
  exec node  "$basedir/../../bin/sanity" "$@"
fi
