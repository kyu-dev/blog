else
  export NODE_PATH="/Users/arthurvienotongaro/Developer/projets/blog/studio/node_modules/.pnpm/@sanity+cli@3.89.0_@types+node@22.15.21_@types+react@18.3.22_react@18.3.1_typescript@5.8.3_yaml@2.8.0/node_modules/@sanity/cli/bin/node_modules:/Users/arthurvienotongaro/Developer/projets/blog/studio/node_modules/.pnpm/@sanity+cli@3.89.0_@types+node@22.15.21_@types+react@18.3.22_react@18.3.1_typescript@5.8.3_yaml@2.8.0/node_modules/@sanity/cli/node_modules:/Users/arthurvienotongaro/Developer/projets/blog/studio/node_modules/.pnpm/@sanity+cli@3.89.0_@types+node@22.15.21_@types+react@18.3.22_react@18.3.1_typescript@5.8.3_yaml@2.8.0/node_modules/@sanity/node_modules:/Users/arthurvienotongaro/Developer/projets/blog/studio/node_modules/.pnpm/@sanity+cli@3.89.0_@types+node@22.15.21_@types+react@18.3.22_react@18.3.1_typescript@5.8.3_yaml@2.8.0/node_modules:/Users/arthurvienotongaro/Developer/projets/blog/studio/node_modules/.pnpm/node_modules:$NODE_PATH"
